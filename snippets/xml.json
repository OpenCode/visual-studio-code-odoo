        ],
        "description": "Odoo Action"
    },
    "odoo_menuitem": {
        "prefix": "menuitem.odoo",
        "body": [
            "<menuitem",
            "\tid=\"$1_menu\"",
            "\tname=\"$2\"",
            "\taction=\"$3\"",
            "\tparent=\"$4\"",
            "\tsequence=\"$5\"/>"
        ],
